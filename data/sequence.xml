<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <record id="seq_patient_record" model="ir.sequence">
        <field name="name">Patient Record Sequence</field>
        <field name="code">patient.record.sequence</field>
        <field name="prefix"></field>
        <field name="padding">6</field>
        <field name="company_id" eval="False"></field>
        </record>

        <record id="seq_appointment_record" model="ir.sequence">
        <field name="name">Appointment Record Sequence</field>
        <field name="code">appointment.record.sequence</field>
        <field name="prefix">AP</field>
        <field name="padding">6</field>
        <field name="company_id" eval="False"></field>
        </record>

        <record id="medical_record_sequence" model="ir.sequence">
            <field name="name">Medical Record Sequence</field>
            <field name="code">medical.record.sequence</field>
            <field name="prefix">IPMR%(year)s/</field>
            <field name="padding">6</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="nursing_assessment_sequence" model="ir.sequence">
            <field name="name">Nursing Assessment Sequence</field>
            <field name="code">nursing.assessment.sequence</field>
            <field name="prefix">NA%(year)s/</field>
            <field name="padding">6</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="doctor_assessment_sequence" model="ir.sequence">
            <field name="name">Doctor Assessment Sequence</field>
            <field name="code">doctor.assessment.sequence</field>
            <field name="prefix">DRA%(year)s/</field>
            <field name="padding">6</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="diagnostic_document_sequence" model="ir.sequence">
            <field name="name">Diagnostic Document</field>
            <field name="code">diagnostic.document.sequence</field>
            <field name="prefix">DOC/%(year)s/%(month)s/</field>
            <field name="padding">6</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</odoo>